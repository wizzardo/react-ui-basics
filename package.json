  "scripts": {
    "preinstall": "",
    "build-css": "sass --no-source-map src:src",
    "watch-css": "npm run build-css && sass --no-source-map --watch src:src",
    "start-js": "SKIP_PREFLIGHT_CHECK=true react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "test": "SKIP_PREFLIGHT_CHECK=true CI=true react-scripts test --env=jsdom",
    "test:watch": "SKIP_PREFLIGHT_CHECK=true react-scripts test --env=jsdom",
    "test:coverage": "npm test -- --coverage",
    "prebuild": "npm run build-css",
    "build": "./build.sh",
    "postinstall": "",
    "clean": "rm -rf build && mkdir build",
    "dist": "NODE_OPTIONS=--openssl-legacy-provider npm-run-all clean build build-storybook && cd build && npm publish",
    "storybook": "NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 9009 -s public",
    "build-storybook": "NODE_OPTIONS=--openssl-legacy-provider build-storybook -c .storybook -o docs"
  },
